alias src="source ~/.zshrc"
alias s="sudo"
alias a="sudo apt-get"
alias cleanBranches="git checkout master && git branch --merged master | egrep -v \"(staging|master)\" | xargs -n 1 git branch -d"
alias listrepositories="egrep -v '^#|^ *$' /etc/apt/sources.list /etc/apt/sources.list.d/*"
alias phantom="phantomjs --webdriver=8643 > /dev/null 2>&1 &"
